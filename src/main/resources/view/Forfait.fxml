      <Button fx:id="btnADD" layoutX="86.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#ajouter" prefHeight="30.0" prefWidth="89.0" style="-fx-background-color: black;" text="Ajouter" textFill="WHITE" />
      <Button fx:id="btnUP" layoutX="225.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#modifier" prefHeight="30.0" prefWidth="89.0" style="-fx-background-color: black;" text="Modifier" textFill="WHITE" />
      <Button fx:id="btnDelete" layoutX="349.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#supprimer" prefHeight="30.0" prefWidth="89.0" style="-fx-background-color: black;" text="Supprimer" textFill="WHITE" />
