<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerForfait">
   <children>
      <Label layoutX="63.0" layoutY="37.0" prefHeight="35.0" prefWidth="104.0" text="Rechercher">
