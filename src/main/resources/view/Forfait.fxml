      <TextField fx:id="btnRS" layoutX="372.0" layoutY="153.0" promptText="Reseaux sociaux" />
      <TableView fx:id="tableForfait" blendMode="EXCLUSION" layoutX="41.0" layoutY="191.0" prefHeight="142.0" prefWidth="504.0">
